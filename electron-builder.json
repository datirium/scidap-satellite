{
    "productName": "scidap-satellite",
    "appId": "com.datirium.scidap-satellite",
    "artifactName": "${productName}-setup-${version}.${ext}",
    "directories": {
        "output": "release/"
    },
    "extraFiles": [
    ],
    "extraResources": [
        "Services/**"
    ],
    "asar": true,
    "asarUnpack": [
    ],
    "files": [
        "**/*",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!Services/",
        "!src/",
        "!utils/",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json"
    ],
    "win": {
        "icon": "dist",
        "target": [
            "portable"
        ]
    },
    "mac": {
        "icon": "dist",
        "target": [
            "dmg"
        ]
    },
    "linux": {
        "icon": "dist",
        "target": [
            "AppImage"
        ]
    }
}